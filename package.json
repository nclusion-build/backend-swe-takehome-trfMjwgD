{
  "name": "backend-swe-takehome-typescript",
  "version": "1.0.0",
  "description": "Backend SWE Take-Home Assignment - typescript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "simulation": "ts-node scripts/simulation.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "supertest": "^6.0.0",
    "@types/supertest": "^2.0.0",
    "sqlite3": "^5.0.0",
    "@types/sqlite3": "^3.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "zod": "^3.0.0",
    "express-rate-limit": "^6.0.0",
    "compression": "^1.7.0",
    "@types/compression": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.15"
  },
  "keywords": [
    "backend",
    "take-home",
    "interview",
    "typescript"
  ],
  "author": "Candidate",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
